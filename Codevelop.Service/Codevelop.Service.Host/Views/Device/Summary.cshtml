
@{
    ViewBag.Title = "Summary";
}

<h2>Heater on Summary</h2>

<style>
    rect.bordered {
        stroke: #E6E6E6;
        stroke-width: 2px;
    }

    text.mono {
        font-size: 9pt;
        font-family: Consolas, courier;
        fill: #aaa;
    }

    text.axis-workweek {
        fill: #000;
    }

    text.axis-worktime {
        fill: #000;
    }
</style>






<div class="days-hours-heatmap">
  
    <!-- heatmap -->
    <div id="heaterchart"></div>
</div>


@section scripts
{
    <script src="~/Scripts/d3.min.js"></script>
    @*<script type="text/javascript" src="http://d3js.org/d3.v3.js"></script>*@
    <script src="~/Scripts/app/deviceSummary.js"></script>
}
